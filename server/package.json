{
  "name": "vedacli",
  "version": "1.0.0",
  "description": "An AI-powered command-line interface built on Gemini. VedaCLI enhances your terminal experience with intelligent command suggestions, automation, and seamless workflow optimization.",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "veda": "./src/cli/main.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "cli": "node src/cli/main.js",
    "dev:link": "chmod +x src/cli/main.js && npm link",
    "preinstall": "chmod +x src/cli/main.js"
  },
  "keywords": [
    "cli",
    "ai",
    "artificial-intelligence",
    "command-line",
    "terminal",
    "automation",
    "productivity",
    "developer-tools",
    "nodejs",
    "javascript",
    "ai-cli",
    "smart-terminal",
    "indian-tech",
    "veda",
    "knowledge"
  ],
  "author": "Soumadip Majila",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.34",
    "@clack/prompts": "^0.11.0",
    "@prisma/client": "^6.19.0",
    "ai": "^5.0.93",
    "better-auth": "^1.3.34",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.10.0",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "prisma": "^6.19.0",
    "yocto-spinner": "^1.0.0",
    "zod": "^4.1.12"
  }
}
